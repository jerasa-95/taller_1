def numero_primo(num):
    
    cont = 0
    
    for i in range(1, num+1):
        if num % i == 0:
            cont += 1
    if cont == 2: 
            return True
    else:
            return False      
  
       
print ('la diferencia es:',num3,
       '¿es este un numero primo? :',numero_primo(num3))

